			}
			description { state: "over" 0.0;
				inherit: "default" 0.0;
				color: 255 255 255 255;
				color2: 51 153 255 128;
				color3: 51 153 255 20;
				color_class: "GLOW_TXT_HIGHLIGHT";
			}
		}
