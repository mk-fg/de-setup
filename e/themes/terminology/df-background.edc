				color2: 51 153 255 18;
				color3: 51 153 255 8;
				color_class: "GLOW_TXT";
				align: 1.0 0.5;
				text { font: "Sans"; size: 8;
					align: 1.0 0.5;
					min: 1 1;
					ellipsis: 0;
