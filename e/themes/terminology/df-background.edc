				rel2.offset: -3 2;
				rel2.to: "viewregion";
				align: 1.0 0.0;
				min: 16 16;
			}
		}
		part { name: "tabcount_ev"; type: RECT; repeat_events: 1;
			clip_to: "tabcount_clip";
			description { state: "default" 0.0;
				fixed: 1 1;
				rel1.to: "terminology.tabcount.control";
				rel2.to: "terminology.tabcount.control";
